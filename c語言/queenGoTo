#include <stdio.h>
#include <stdlib.h>


void QueenGoLeft(int** checkerBoard,int queenPosX,int queenPosY,int offsetX,int offsetY){
	queenPosX+=offsetX;
	queenPosY+=offsetY;
	if(checkerBoard[queenPosX][queenPosY]==-1)return;
	checkerBoard[queenPosX][queenPosY]=1;
	return QueenGoLeft(checkerBoard,queenPosX,queenPosY,offsetX,offsetY);
}


int main(){
	int**checkerBoard = (int**)calloc(10,sizeof(int*)*10);
	for(int i=0;i<10;i++){
		checkerBoard[i] = (int*)calloc(10,sizeof(int)*10);
	}
	
	for(int i=0;i<10;i++){
		for(int j=0;j<10;j++){
			if(i==0 || j==0 || i==9 || j==9){
				checkerBoard[i][j]=-1;
			}
		}
	}
	
	
	int inputX,inputY;
	scanf("%d %d",&inputX,&inputY);
	
	checkerBoard[inputX+1][inputY+1]=2;
	
	//上
	QueenGoLeft(checkerBoard,inputX,inputY,0,-1);
	//下 
	QueenGoLeft(checkerBoard,inputX,inputY,0,1);
	//左 
	QueenGoLeft(checkerBoard,inputX,inputY,-1,0);
	//右 
	QueenGoLeft(checkerBoard,inputX,inputY,1,0);
	//左上 
	QueenGoLeft(checkerBoard,inputX,inputY,-1,-1);
	//右上 
	QueenGoLeft(checkerBoard,inputX,inputY,1,-1);
	//左下 
	QueenGoLeft(checkerBoard,inputX,inputY,-1,1);
	//右下  
	QueenGoLeft(checkerBoard,inputX,inputY,1,1);
	
	
	
	
	
	
	for(int i=0+1;i<10-1;i++){
		for(int j=0+1;j<10-1;j++){
			if(checkerBoard[i][j]==2){
				printf("Q ");
				continue;
			}
			printf("%d ",checkerBoard[i][j]);
		}
		printf("\n");
	}
}
